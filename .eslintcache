y} />\n      <div className={styles.right_menu}>\n        {LoginCompleted ? (\n          <Logout changeHeaderButton={changeHeaderButton} />\n        ) : (\n          <SignIn renderLoginModal={renderLoginModal} />\n        )}\n        {LoginCompleted ? (\n          <UserInfo renderUserInfoModal={renderUserInfoModal} />\n        ) : (\n          <SignUp renderRegisterModal={renderRegisterModal} />\n        )}\n      </div>\n      {Login && (\n        <LoginModal\n          renderLoginModal={renderLoginModal}\n          renderRegisterModal={renderRegisterModal}\n          changeHeaderButton={changeHeaderButton}\n        />\n      )}\n      {Register && <RegisterModal renderRegisterModal={renderRegisterModal} />}\n      {UserInformation && (\n        <UserInfoModal\n          renderUserInfoModal={renderUserInfoModal}\n          renderEditUserModal={renderEditUserModal}\n        />\n      )}\n\n      {EditUserForm && (\n        <EditUserModal\n          renderUserInfoModal={renderUserInfoModal}\n          renderEditUserModal={renderEditUserModal}\n        />\n      )}\n    </div>\n  );\n}\n","/home/yeongbba/Git/project/4-scrapers-client/src/components/views/StoragePage/StoragePage.jsx",["117"],"import React from 'react';\nimport MainPage from '../MainPage/MainPage';\nimport styles from './StoragePage.module.css';\n\nconst categoryList = ['전체', '정치', '연예', '스포츠', '사회', '생활/문화', '세계', 'IT/과학', '기타' ];\n\nexport default function StoragePage() {\n    return(\n        <div className={styles.container}>\n          {/* 카테고리 목록 */}\n          <div className={styles.categoryList}>\n            {categoryList.map(item => {\n                return <div className={styles.category}>{item}</div>\n            })}\n          </div>\n          {/* <MainPage /> */}\n        </div>\n    )\n}","/home/yeongbba/Git/project/4-scrapers-client/src/service/user.js",[],"/home/yeongbba/Git/project/4-scrapers-client/src/_actions/types.js",[],"/home/yeongbba/Git/project/4-scrapers-client/src/component